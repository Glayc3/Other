age = int(input("how many years old are you"))
months = age*12
days = age*365
hours = days*24
minutes = hours*60
seconds = minutes*60
milliseconds = seconds*1000

print("you have lived for a total of:")
print(age, " years")
print(months, " months")
print(days, " days")
print(hours, " hours")
print(minutes, " minutes")
print(seconds, " seconds")
print(milliseconds, " milliseconds")
